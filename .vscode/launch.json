{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run FastAPI (Uvicorn)",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "resultats.repository.geoparquet_app.main:app",
        "--reload"
      ],
      "jinja": true
    },
    {
      "name": "Run Streamlit App",
      "type": "python",
      "request": "launch",
      "module": "streamlit",
      "args": [
        "run",
        "resultats/repository/geoparquet_app/main.py"
      ]
    }
  ]
}